{
  "name": "gravitaesql",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist src/generated",
    "build": "yarn clean && yarn generate && tsc",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "migrate": "yarn migrate:prisma && yarn migrate:db-migrate",
    "migrate:prisma": "prisma migrate up --experimental",
    "migrate:db-migrate": "db-migrate up fix-data-types",
    "test": "jest",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "firebase": "yarn firebase:execute && yarn firebase:checker",
    "firebase:execute": "ts-node --transpile-only src/scripts/firebaseMigration",
    "firebase:checker": "ts-node --transpile-only src/scripts/firebaseMigrationChecker"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "jest": "^26.2.2",
    "md5-typescript": "^1.0.5",
    "moment": "^2.27.0",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@nexus/schema": "^0.15.0",
    "@types/node-fetch": "^2.5.7",
    "apollo-server": "^2.16.1",
    "db-migrate": "^0.11.11",
    "db-migrate-mysql": "^2.1.1",
    "firebase-admin": "^9.0.0",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.2.6",
    "nexus-plugin-prisma": "^0.16.1",
    "ntp-ulid": "^1.0.1",
    "sort-keys": "^4.0.0"
  }
}
